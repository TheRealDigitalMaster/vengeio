#transition{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0px;
	top: 0px;
	background: #151515;
	z-index: 1000;
	animation: fadeOut 0.5s ease-out;
	animation-fill-mode: forwards;
}

#transition.active{
	animation: fadeIn 0.5s ease-out;
	animation-fill-mode: forwards;
}

@keyframes fadeIn {
    0% {
        opacity: 0;
    }

    100% {
        opacity: 1;
    }
}

@keyframes fadeOut {
    0% {
        opacity: 1;
    }

    100% {
        opacity: 0;
    }
}

