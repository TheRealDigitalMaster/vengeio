@import "profile.css";
@import "table.css";
@import "form.css";
@import "loadout.css";
@import "clan.css";
@import "chat.css";


html, body {
	height: 100%;
	margin: 0px;
	padding: 0px;
	background: #3d3d3d url('../images/Blur-Background.jpg') no-repeat;
	background-size: cover;
	font-family: 'Roboto Condensed';
	-webkit-tap-highlight-color: transparent;
	overflow: hidden;
}

div, span, html, body {
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

#full-screen-loading {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 100%;
	background: #272727;
	z-index: 5000;
	background-size: cover;
	background-position: center center;
}

#loading-bar{
	position: absolute;
	left: 20px;
	bottom: 40px;
	width: calc(100% - 40px);
	height: 10px;
	border: solid 2px #000;
	background-color: #666;
	border-radius: 3px;
	overflow: hidden;
}

#loading-bar .loading-bar-fill{
	width: 0%;
	height: 10px;
	background-color: rgb(255, 191, 0);
	animation: loading-bar-fill 15s linear;
	animation-fill-mode: forwards;
}

#tips{
	padding: 10px 20px;
	font-size: 1.2vw;
	background-color: rgba(0, 0, 0, 0.5);
	position: absolute;
	left: 20px;
	bottom: 80px;
	color: #fff;
	border-radius: 3px;
}

@keyframes loading-bar-fill {
	0% {
		width: 0%;
	}
	100% {
		width: 100%;
	}
}

#full-screen-loading .loader {
	position: absolute;
	left: calc(50% - 30px);
	top: calc(50% - 30px);
}

#half-screen-loading {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.5);
	z-index: 1000;
}

#half-screen-loading .loader {
	position: absolute;
	left: calc(50% - 30px);
	top: calc(50% - 30px);
}

.loader-container {
	position: absolute;
	right: 25%;
	top: 50%;
	transform: translate(-25%, -50%);
}

#application-canvas {
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
}

#application-canvas.fill-mode-NONE {
	margin: auto;
}

#application-canvas.fill-mode-KEEP_ASPECT {
	width: 100%;
	height: auto;
	margin: 0;
}

#application-canvas.fill-mode-FILL_WINDOW {
	width: 100%;
	height: 100%;
	margin: 0;
}

canvas:focus {
	outline: none;
}

::-webkit-scrollbar {
	width: 1vw;
}


.rainbow {
    background: linear-gradient(to right, #6666ff, #0099ff , #00ff00, #ff3399, #f80e0e, #82c4fa, #6666ff);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
    animation: rainbow_animation 5s ease-in-out infinite;
    background-size: 400% 100%;
}

@keyframes rainbow_animation {
    0%,100% {
        background-position: 0 0;
    }

    50% {
        background-position: 100% 0;
    }
}

/* Track */
::-webkit-scrollbar-track {
	background: rgba(0, 0, 0, 0.1);
}

/* Handle */
::-webkit-scrollbar-thumb {
	background: #888;
}

/* Handle on hover */
::-webkit-scrollbar-thumb:hover {
	background: #555;
}

.info{
	width: fit-content;
    padding: 10px;
    border: 1px solid #dedede;
    background: rgb(255 201 2 / 76%);
    color: #fff;
    text-shadow: 2px 2px 2px #00000054;
    border-radius: 5px;
    margin-bottom: 20px;
}

.info p{
