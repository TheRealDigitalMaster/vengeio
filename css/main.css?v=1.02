.back-button-wrap {
	position: relative;
	float: left;
	width: 100%;
	max-width: 60px;
	color: #fff;
	text-decoration: none;
	margin: 0 20px 0 0;
	background: rgb(72 71 71 / 80%);
	overflow: hidden;
	transition: ease max-width 0.5s;
}

.back-button {
	width: 38px;
	height: 38px;
	float: left;
	margin-right: 25px;
	margin-top: 0px;
	margin-left: 0px;
	background: rgb(66 66 66 / 70%);
	padding: 11px;
	cursor: pointer;
}

.back-button-wrap:hover,
.back-button:hover {
	background: rgba(0, 0, 0, 0.1);
}

.back-button-wrap:hover {
	max-width: 160px;
}

.back-button-wrap .back-button {
	float: right;
	margin-right: 0;
	padding-left: 10px;
	background: initial;
	z-index: 1;
}

.back-button-wrap p {
	position: absolute;
	left: -100%;
	transform: translateX(-50%);
	font-size: 18px;
	padding: 0 25px;
	opacity: 0;
	margin: 0;
	pointer-events: none;
	transition-delay: 0.2s;
	transition: ease opacity 0.4s;
}

.back-button-wrap:hover p {
	opacity: 1;
	transform: translateX(calc(-50% + 30px));
	left: calc(50% - 50px);
	pointer-events: initial;
}



